{
    "name": "dmk/dmkclub",
    "description": "DMKClub",
    "homepage": "https://github.com/DMKEBUSINESSGMBH/DMKClub.git",
    "license": "OSL-3.0",
    "authors": [
        {
            "name": "DMK E-BUSINESS GmbH",
            "homepage": "https://www.dmk-ebusiness.de"
        }
    ],
    "autoload": {
        "psr-4": {
            "DMKClub\\Bundle\\": "src/DMKClub/Bundle/"
        }
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://packagist.orocrm.com"
        }
    ],
    "require": {
        "php": "^8.3",
        "digitick/sepa-xml": "^2.2",
        "ehdev/oro-basics-bundle": "6.0.x-dev",
        "qipsius/tcpdf-bundle": "^3.0",
        "oro/crm": "^6.0"
    },
    "require-dev": {
        "composer/semver": "dev-main as 3.3.2",
        "friendsofphp/php-cs-fixer": "^3.39.1",
        "kubawerlos/php-cs-fixer-custom-fixers": "^3.18",
        "phpspec/prophecy-phpunit": "^2.2",
        "phpstan/extension-installer": "^1.3",
        "phpstan/phpstan": "^1.10",
        "phpstan/phpstan-doctrine": "^1.3",
        "phpstan/phpstan-phpunit": "^1.3",
        "phpunit/phpunit": "^9.5.0"
    },
    "config": {
        "sort-packages": true,
        "vendor-dir": ".Build/vendor",
        "bin-dir": ".Build/bin",
        "allow-plugins": {
            "symfony/runtime": true,
            "symfony/flex": true,
            "php-http/discovery": true,
            "phpstan/extension-installer": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
